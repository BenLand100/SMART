program EIOSTest;

procedure InitSmart();
var
  version: integer; //version of this record
  root, params: string; //Passed to SmartSetup
  width, height: integer; //Passed to SmartSetup
  initseq: string; //Passed to SmartSetup
  maxmem: integer; //Max JVM mem in MB, -1 is default
  jvmpath, useragent: string; //'' is the old defaults
begin
  version:= 0;
  root:= 'http://world15.runescape.com/';
  params:= 'plugin.js?param=o0,a1,m0';
  width:= 765;
  height:= 503;
  initseq:= 's';
  maxmem:= -1; //use SMART's default
  jvmpath:= ''; //use SMART's default
  useragent:= ''; //use SMART's default
  SetEIOSTarget('libsmart',IntToStr(version)+',"'+root+'","'+params+'",'+IntToStr(width)+','+IntToStr(height)+',"'+initseq+'",'+IntToStr(maxmem)+',"'+jvmpath+'","'+useragent+'"');
end;

begin
  InitSmart();
  //Now, all of Simba's functions use SMART as the target, no hacks.
  while true do
  begin
    MoveMouse(Random(765),Random(503));
    Wait(1000);
  end;
end.
